{
  "name": "rxjs-cheatsheet",
  "displayName": "RxJS Cheatsheet",
  "description": "RxJS cheatsheet inside VS Code",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": [
    "RxJS",
    "Cheatsheet",
    "Angular",
    "Vue",
    "Svelte",
    "React"
  ],
  "license": "MIT",
  "icon": "assets/images/icon.jpg",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dzhavat/rxjs-cheatsheet"
  },
  "publisher": "dzhavat",
  "main": "./out/extension.js",
  "vsce": {
    "githubBranch": "main"
  },
  "contributes": {
    "commands": [
      {
        "command": "rxjs.cheatsheet",
        "title": "Open RxJS Cheatsheet"
      }
    ]
  },
  "activationEvents": [
    "onCommand:rxjs.cheatsheet"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.14.31",
    "@types/vscode": "1.51.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4",
    "vscode-test": "^1.6.1"
  }
}
