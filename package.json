{
	"name": "rxjs-cheatsheet",
	"displayName": "RxJS Cheatsheet",
	"description": "RxJS cheatsheet inside VS Code",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"keywords": [
		"RxJS",
		"Cheatsheet",
		"Angular",
		"Vue",
		"Svelte",
		"React"
	],
	"license": "MIT",
	"icon": "assets/images/icon.jpg",
	"repository": {
		"type": "git",
		"url": "https://github.com/dzhavat/rxjs-cheatsheet"
	},
	"publisher": "dzhavat",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rxjs.cheatsheet",
				"title": "Open RxJS Cheatsheet"
			}
		]
	},
	"activationEvents": [
		"onCommand:rxjs.cheatsheet"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "^12.12.0",
		"@types/vscode": "1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"eslint": "^7.28.0",
		"glob": "^7.1.7",
		"mocha": "^9.0.0",
		"typescript": "^4.2.4",
		"vscode-test": "^1.5.2"
	}
}
